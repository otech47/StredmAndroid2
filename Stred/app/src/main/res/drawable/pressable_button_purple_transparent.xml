<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:drawable="@color/setmine_purple_95_alpha"></item>
    <item android:state_pressed="false" android:drawable="@color/transparent"></item>
    <item>
        <shape
            android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/setmine_purple_75_alpha" />
            <solid android:color="@color/setmine_white" />

        </shape>
    </item>

    <item android:top="1dp" android:bottom="1dp">
        <shape
            android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/setmine_white" />
            <solid android:color="#00000000" />
        </shape>
    </item>
</selector>