<resources>

    <!-- Base application theme. -->
    <style name="SetMine" parent="android:Theme.Holo">
        <!-- Customize your theme here. -->
        <item name="android:windowActionBarOverlay">false</item>
        <item name="android:actionBarStyle">@style/CustomActionBar</item>
        <item name="android:windowActionBar">true</item>
    </style>

    <style name="CustomActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/setmine_blue</item>
        <item name="android:displayOptions">disableHome</item>
    </style>

    <style name="StyledIndicators" parent="@android:style/Theme.Light">
        <item name="vpiTitlePageIndicatorStyle">@style/CustomTitlePageIndicator</item>
    </style>

    <style name="CustomTitlePageIndicator">

    </style>

    <style name="RelativeLayout">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>


</resources>
